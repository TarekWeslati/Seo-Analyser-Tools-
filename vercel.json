{
  "version": 2,
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "frontend/public/static/$1"
    },
    {
      "src": "/",
      "dest": "frontend/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "backend/app.py"
    }
  ]
}
